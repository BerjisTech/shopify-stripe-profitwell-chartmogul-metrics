

<h1>Events</h1>

<table class="table table-responsive table-hover">
  <thead class="table-dark bg-dark">
    <tr>
      <th>App</th>
      <th>Website</th>
      <th>Email</th>
      <th>Event</th>
      <th>Time</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.app_id %></td>
        <td><%= event.website %></td>
        <td><%= event.email %></td>
        <td><%= event.event %></td>
        <td><%= event.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
