<script>
  function inform(info, type) {
      var opts = {
          "closeButton": true,
          "debug": false,
          "positionClass": "toast-top-right",
          "toastClass": "primary",
          "onclick": null,
          "showDuration": "300",
          "hideDuration": "1000",
          "timeOut": "10000",
          "extendedTimeOut": "1000",
          "showEasing": "swing",
          "hideEasing": "linear",
          "showMethod": "fadeIn",
          "hideMethod": "fadeOut"
      };
      if (type == 'Error') {
          toastr.error(info, "Error:", opts)
      }
      else {
          toastr.info(info, "Info:", opts)
      }
  }
</script>
