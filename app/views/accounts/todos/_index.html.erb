<div class="account_block">
  <div class="account_block_title">
    <span>ToDos</span>
    <span class="material-icons">list</span>
  </div>
  <div class="account_block_content">
    <form class="to_do_content_form d-flex flex-row">
      <input type="text" name="todo[to_do]" required placeholder="Add your todo here" />
      <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
      <input type="date" name="todo[deadline]" />
    </form>
  </div>
</div>
<%= render "accounts/todos/js" %>
<script>
  load_todos()
</script>
